{
  "name": "react-vite-ssr",
  "description": "React pattern with vite and ssr page.",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "React",
    "React router",
    "React router dom",
    "React redux",
    "Redux & toolkit",
    "Vite",
    "env",
    "SSR"
  ],
  "author": "Alexander Boev",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development && vite dev",
    "dev:mock": "concurrently \"vite dev\" \"npm run mock\"",
    "prod": "vite build && vite preview",
    "prod:mock": "concurrently \"vite build && vite preview\" \"npm run mock\"",
    "mock": "json-server --watch public/database/db.json -p 4301 -d 0",
    "build": "vite build"
  },
  "dependencies": {
    "primeicons": "^6.0.1",
    "primereact": "^10.0.3"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "json-server": "^0.17.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "redux": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vike": "^0.4.143",
    "vite": "^4.4.11"
  }
}
